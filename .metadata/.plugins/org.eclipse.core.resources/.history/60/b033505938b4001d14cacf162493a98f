package com.aurionpro.model;

public class Loader implements IWorker {

	@Override
	public void start() {
System.out.println("worker is started");
	}

	@Override
	public void stop() {
		System.out.println("worker is stop");
	}

	@Override
	public void eat() {
		System.out.println("worker is eating");
	}

	@Override
	public void rest() {
		System.out.println("worker takes a rest");
	}

}
